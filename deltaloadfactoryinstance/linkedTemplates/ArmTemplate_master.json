{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "deltaloadfactoryinstance"
		},
		"watermarkdbLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'watermarkdbLinkedService'"
		},
		"sourcedblinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sourcedblinkedservice'"
		},
		"destinationstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'destinationstorage'"
		},
		"watermarkdbLinkedService_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "anubhatayal.database.windows.net"
		},
		"watermarkdbLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureLearning1"
		},
		"watermarkdbLinkedService_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SQLSERVER_AT"
		},
		"sourcedblinkedservice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "anubhatayal.database.windows.net"
		},
		"sourcedblinkedservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AzureLearning1"
		},
		"sourcedblinkedservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SQLSERVER_AT"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "deltaloadfactoryinstance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"watermarkdbLinkedService_password": {
						"value": "[parameters('watermarkdbLinkedService_password')]"
					},
					"sourcedblinkedservice_password": {
						"value": "[parameters('sourcedblinkedservice_password')]"
					},
					"destinationstorage_connectionString": {
						"value": "[parameters('destinationstorage_connectionString')]"
					},
					"watermarkdbLinkedService_properties_typeProperties_server": {
						"value": "[parameters('watermarkdbLinkedService_properties_typeProperties_server')]"
					},
					"watermarkdbLinkedService_properties_typeProperties_database": {
						"value": "[parameters('watermarkdbLinkedService_properties_typeProperties_database')]"
					},
					"watermarkdbLinkedService_properties_typeProperties_userName": {
						"value": "[parameters('watermarkdbLinkedService_properties_typeProperties_userName')]"
					},
					"sourcedblinkedservice_properties_typeProperties_server": {
						"value": "[parameters('sourcedblinkedservice_properties_typeProperties_server')]"
					},
					"sourcedblinkedservice_properties_typeProperties_database": {
						"value": "[parameters('sourcedblinkedservice_properties_typeProperties_database')]"
					},
					"sourcedblinkedservice_properties_typeProperties_userName": {
						"value": "[parameters('sourcedblinkedservice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}